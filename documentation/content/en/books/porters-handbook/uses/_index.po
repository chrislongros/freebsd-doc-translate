msgstr ""

#. type: Plain text
#: documentation/content/en/books/porters-handbook/uses/_index.adoc:1609
msgid "`demo`"
msgstr ""

#. type: Plain text
#: documentation/content/en/books/porters-handbook/uses/_index.adoc:1610
msgid "`designer`"
msgstr ""

#. type: Plain text
