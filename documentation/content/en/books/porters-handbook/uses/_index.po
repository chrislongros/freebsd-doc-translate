msgstr ""

#. type: Plain text
#: documentation/content/en/books/porters-handbook/uses/_index.adoc:1424
msgid "`demo`"
msgstr ""

#. type: Plain text
#: documentation/content/en/books/porters-handbook/uses/_index.adoc:1425
msgid "`designer`"
msgstr ""

#. type: Plain text
