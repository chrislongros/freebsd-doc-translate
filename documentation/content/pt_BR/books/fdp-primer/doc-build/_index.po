msgstr ""
"PYTHON_CMD =\t/usr/local/bin/python3 <.>\n"
"HUGO_CMD =\t/usr/local/bin/hugo <.>\n"
"LANGUAGES =\ten,es,pt_BR,de,ja,zh_CN,zh_TW,ru,el,hu,it,mn,nl,pl,fr <.>\n"
"RUBYLIB =\t../shared/lib\n"
".export\tRUBYLIB\n"

